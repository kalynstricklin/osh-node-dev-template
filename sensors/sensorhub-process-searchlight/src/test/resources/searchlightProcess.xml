<sml:AggregateProcess xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:sml="http://www.opengis.net/sensorml/2.0" xmlns:swe="http://www.opengis.net/swe/2.0" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:gco="http://www.isotc211.org/2005/gco" xmlns:gmd="http://www.isotc211.org/2005/gmd" gml:id="F1">
  <gml:identifier codeSpace="uid">a2574263-3817-4b2b-913d-73a56ae6c106</gml:identifier>
  <sml:outputs>
    <sml:OutputList>
      <sml:output name="SearchlightColor">
        <swe:Category definition="http://sensorml.com/ont/swe/property/Color">
          <swe:label>searchlight</swe:label>
        </swe:Category>
      </sml:output>
    </sml:OutputList>
  </sml:outputs>
  <sml:components>
    <sml:ComponentList>
      <sml:component name="source0">
        <sml:SimpleProcess gml:id="F2">
          <sml:typeOf xlink:href="urn:osh:process:datasource:stream"/>
          <sml:configuration>
            <sml:Settings>
              <sml:setValue ref="parameters/producerURI">urn:osh:sensor:universalcontroller</sml:setValue>
            </sml:Settings>
          </sml:configuration>
        </sml:SimpleProcess>
      </sml:component>
      <sml:component name="process0">
        <sml:SimpleProcess gml:id="F3">
          <sml:typeOf xlink:href="urn:osh:process:pi-bot:searchlight"/>
          <sml:inputs>
            <sml:InputList>
              <sml:input name="buttonA">
                <swe:Quantity>
                  <swe:label>Update Color</swe:label>
                  <swe:uom xlink:href="http://qudt.org/vocab/unit/UNITLESS"/>
                </swe:Quantity>
              </sml:input>
            </sml:InputList>
          </sml:inputs>
          <sml:outputs>
            <sml:OutputList>
              <sml:output name="process">
                <swe:DataRecord updatable="true" definition="http://sensorml.com/ont/swe/property/SearchlightProcess">
                  <swe:field name="Color">
                    <swe:Category definition="http://sensorml.com/ont/swe/property/Color">
                      <swe:constraint>
                        <swe:AllowedTokens>
                          <swe:value>OFF</swe:value>
                          <swe:value>WHITE</swe:value>
                          <swe:value>RED</swe:value>
                          <swe:value>GREEN</swe:value>
                          <swe:value>MAGENTA</swe:value>
                          <swe:value>BLUE</swe:value>
                          <swe:value>YELLOW</swe:value>
                          <swe:value>CYAN</swe:value>
                        </swe:AllowedTokens>
                      </swe:constraint>
                    </swe:Category>
                  </swe:field>
                </swe:DataRecord>
              </sml:output>
            </sml:OutputList>
          </sml:outputs>
        </sml:SimpleProcess>
      </sml:component>
      <sml:component name="sink0">
        <sml:SimpleProcess gml:id="F4">
          <sml:typeOf xlink:href="urn:osh:process:datasink:commandstream"/>
          <sml:configuration>
            <sml:Settings>
              <sml:setValue ref="parameters/systemUID">urn:pibot:searchlight:rgb-searchlight</sml:setValue>
              <sml:setValue ref="parameters/inputName">SearchlightControl</sml:setValue>
            </sml:Settings>
          </sml:configuration>
        </sml:SimpleProcess>
      </sml:component>
    </sml:ComponentList>
  </sml:components>
  <sml:connections>
    <sml:ConnectionList>
      <sml:connection>
        <sml:Link>
          <sml:source ref="components/source0/outputs/output1/gamepads/gamepad0/A"/>
          <sml:destination ref="components/process0/inputs/buttonA"/>
        </sml:Link>
      </sml:connection>
      <sml:connection>
        <sml:Link>
          <sml:source ref="components/process0/outputs/process/Color"/>
          <sml:destination ref="components/sink0/inputs/SearchlightControl/Color"/>
        </sml:Link>
      </sml:connection>
      <sml:connection>
        <sml:Link>
          <sml:source ref="components/process0/outputs/process/Color"/>
          <sml:destination ref="outputs/SearchlightColor"/>
        </sml:Link>
      </sml:connection>
    </sml:ConnectionList>
  </sml:connections>
</sml:AggregateProcess>