<sml:AggregateProcess xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:sml="http://www.opengis.net/sensorml/2.0" xmlns:swe="http://www.opengis.net/swe/2.0" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:gco="http://www.isotc211.org/2005/gco" xmlns:gmd="http://www.isotc211.org/2005/gmd" gml:id="F1">
    <gml:identifier codeSpace="uid">1e8fc1b0-a655-44dc-8c2e-9b2cf4f3950c</gml:identifier>
    <sml:typeOf xlink:href="urn:osh:process:pi-bot:searchlight"/>
    <sml:inputs>
        <sml:InputList>
            <sml:input name="buttons">
                <sml:ObservableProperty definition="http://sensorml.com/ont/swe/property/Color"/>
            </sml:input>
            <sml:input name="button1" xlink:href="https://www.nintendo.com/us/store/products/wii-remotee"/>
        </sml:InputList>
    </sml:inputs>
    <sml:outputs>
        <sml:OutputList>
            <sml:output name="color">
                <swe:DataRecord>
                    <swe:label>Searchlight Data Record</swe:label>
                    <swe:description>Pi-bot searchlight process</swe:description>
                    <swe:field name="time">
                        <swe:Time definition="http://www.opengis.net/def/property/OGC/0/SamplingTime" referenceFrame="http://www.opengis.net/def/trs/BIPM/0/UTC">
                            <swe:label>Sampling Time</swe:label>
                            <swe:uom xlink:href="http://www.opengis.net/def/uom/ISO-8601/0/Gregorian"/>
                        </swe:Time>
                    </swe:field>
                    <swe:field name="color">
                        <swe:Category definition="http://sensorml.com/ont/swe/property/Color">
                            <swe:label>RGBColor</swe:label>
                            <swe:constraint>
                                <swe:AllowedTokens>
                                    <swe:value>OFF</swe:value>
                                    <swe:value>WHITE</swe:value>
                                    <swe:value>RED</swe:value>
                                    <swe:value>MAGENTA</swe:value>
                                    <swe:value>BLUE</swe:value>
                                    <swe:value>CYAN</swe:value>
                                    <swe:value>GREEN</swe:value>
                                    <swe:value>YELLOW</swe:value>
                                    <swe:value>UNKNOWN</swe:value>
                                </swe:AllowedTokens>
                            </swe:constraint>
                        </swe:Category>
                    </swe:field>
                </swe:DataRecord>
            </sml:output>
            <sml:output name="status_info" xlink:href=""/>
        </sml:OutputList>
    </sml:outputs>
    <sml:parameters>
        <sml:ParameterList>
            <sml:parameter name="samplingPeriod">
                <swe:Quantity definition="http://sensorml.com/ont/swe/property/SamplingPeriod">
                    <swe:label>Sampling Period</swe:label>
                    <swe:uom code="s"/>
                </swe:Quantity>
            </sml:parameter>
        </sml:ParameterList>
    </sml:parameters>
    <sml:components>
        <sml:ComponentList>
            <sml:component name="searchlight1">
                <sml:PhysicalComponent gml:id="SEARCHLIGHT01">
                    <sml:typeOf xlink:href="http://www.yahboom.net/study/G1-T-PI "/>
                    <sml:configuration>
                        <sml:Settings>
                            <sml:setValue ref="parameters/samplingRate">1.0</sml:setValue>
                            <sml:setMode ref="modes/choice1">highAccuracy</sml:setMode>
                            <sml:setStatus ref="parameters/active">enabled</sml:setStatus>
                        </sml:Settings>
                    </sml:configuration>
                    <sml:outputs>
                        <sml:OutputList>
                            <sml:output name="colorLight">
                                <swe:Category/>
                            </sml:output>
                        </sml:OutputList>
                    </sml:outputs>
                </sml:PhysicalComponent>
            </sml:component>
        </sml:ComponentList>
    </sml:components>
    <sml:connections>
        <sml:ConnectionList>
            <sml:connection>
                <sml:Link>
                    <sml:source ref="components/sensor1/outputs/colorLight"/>
                    <sml:destination ref="outputs/color/colorLight"/>
                </sml:Link>
            </sml:connection>
            <sml:connection>
                <sml:Link>
                    <sml:source ref="components/sensor1/inputs/button1"/>
                    <sml:destination ref="inputs/buttons/button1"/>
                </sml:Link>
            </sml:connection>
        </sml:ConnectionList>
    </sml:connections>
</sml:AggregateProcess>